<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <com.google.android.material.card.MaterialCardView
        android:id="@+id/popup_schedule_cv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:cardElevation="3dp"
        app:cardCornerRadius="4dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/popup_schedule_adjust_lo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingBottom="14dp"
                android:paddingEnd="20dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent">
                <ImageView
                    android:id="@+id/popup_schedule_adjust_iv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_trash_can"
                    app:tint="@color/line_black"
                    android:layout_marginTop="16dp"
                    android:layout_marginStart="20dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>
                <TextView
                    android:id="@+id/popup_schedule_adjust_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/min_sans_medium"
                    android:text="수정"
                    android:textSize="14sp"
                    android:textColor="@color/line_black"
                    android:layout_marginStart="4dp"
                    app:layout_constraintTop_toTopOf="@id/popup_schedule_adjust_iv"
                    app:layout_constraintBottom_toBottomOf="@id/popup_schedule_adjust_iv"
                    app:layout_constraintStart_toEndOf="@id/popup_schedule_adjust_iv"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/popup_schedule_remove_lo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingBottom="16dp"
                android:paddingEnd="20dp"
                app:layout_constraintTop_toBottomOf="@id/popup_schedule_adjust_lo"
                app:layout_constraintStart_toStartOf="parent">
                <ImageView
                    android:id="@+id/popup_schedule_remove_iv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_trash_can"
                    app:tint="@color/line_black"
                    android:layout_marginTop="14dp"
                    android:layout_marginStart="20dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>
                <TextView
                    android:id="@+id/popup_schedule_remove_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/min_sans_medium"
                    android:text="삭제"
                    android:textSize="14sp"
                    android:textColor="@color/line_black"
                    android:layout_marginStart="4dp"
                    app:layout_constraintTop_toTopOf="@id/popup_schedule_remove_iv"
                    app:layout_constraintBottom_toBottomOf="@id/popup_schedule_remove_iv"
                    app:layout_constraintStart_toEndOf="@id/popup_schedule_remove_iv"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </com.google.android.material.card.MaterialCardView>

</androidx.constraintlayout.widget.ConstraintLayout>