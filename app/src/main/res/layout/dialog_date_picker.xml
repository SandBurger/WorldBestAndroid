<?xml version="1.0" encoding="utf-8"?>
        <com.google.android.material.card.MaterialCardView
            xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:elevation="0dp"
            app:cardCornerRadius="8dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent">
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:id="@+id/dialog_date_picker_header_lo"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@color/background_white"
                            app:layout_constraintTop_toTopOf="parent">
                                <androidx.appcompat.widget.AppCompatImageButton
                                    android:id="@+id/dialog_date_picker_previous_ib"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:src="@drawable/ic_left_sign"
                                    android:background="@android:color/transparent"
                                    android:layout_marginStart="21dp"
                                    app:layout_constraintTop_toTopOf="parent"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintStart_toStartOf="parent"/>
                                <TextView
                                    android:id="@+id/dialog_date_picker_date_tv"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/min_sans_medium"
                                    android:text="2022년 5월"
                                    android:textSize="20sp"
                                    android:textColor="@color/line_black"
                                    android:layout_marginTop="12dp"
                                    android:paddingBottom="10dp"
                                    app:layout_constraintStart_toEndOf="@id/dialog_date_picker_previous_ib"
                                    app:layout_constraintEnd_toStartOf="@id/dialog_date_picker_next_ib"
                                    app:layout_constraintTop_toTopOf="@id/dialog_date_picker_previous_ib"
                                    app:layout_constraintBottom_toBottomOf="@id/dialog_date_picker_previous_ib"/>
                                <androidx.appcompat.widget.AppCompatImageButton
                                    android:id="@+id/dialog_date_picker_next_ib"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:src="@drawable/ic_right_sign"
                                    android:background="@android:color/transparent"
                                    android:layout_marginEnd="21dp"
                                    app:layout_constraintTop_toTopOf="@id/dialog_date_picker_previous_ib"
                                    app:layout_constraintBottom_toBottomOf="@id/dialog_date_picker_previous_ib"
                                    app:layout_constraintEnd_toEndOf="parent"/>

                        </androidx.constraintlayout.widget.ConstraintLayout>

                        <androidx.fragment.app.FragmentContainerView
                            android:id="@+id/dialog_date_picker_date_container"
                            android:name="com.example.sandiary.ui.calendar.FragmentCalendarDate"
                            android:layout_width="0dp"
                            android:layout_height="30dp"
                            android:layout_marginTop="3dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@id/dialog_date_picker_header_lo" />
                        <com.kizitonwose.calendarview.CalendarView
                            android:id="@+id/dialog_date_picker_calendar_cv"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:cv_orientation="horizontal"
                            app:cv_inDateStyle="allMonths"
                            app:cv_outDateStyle="endOfRow"
                            app:cv_scrollMode="paged"
                            app:cv_hasBoundaries="true"
                            app:cv_dayViewResource="@layout/item_add_schedule_day"
                            app:layout_constraintTop_toBottomOf="@id/dialog_date_picker_date_container"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"/>
                        <TextView
                            android:id="@+id/dialog_date_picker_confirm_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/min_sans_medium"
                            android:text="확인"
                            android:textSize="14sp"
                            android:textColor="@color/active"
                            android:layout_marginEnd="17dp"
                            android:layout_marginTop="34dp"
                            android:paddingBottom="20dp"
                            app:layout_constraintTop_toBottomOf="@id/dialog_date_picker_calendar_cv"
                            app:layout_constraintEnd_toEndOf="parent"/>
                        <TextView
                            android:id="@+id/dialog_date_picker_cancel_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="32dp"
                            android:fontFamily="@font/min_sans_medium"
                            android:text="취소"
                            android:textSize="14sp"
                            android:textColor="@color/active"
                            app:layout_constraintTop_toTopOf="@id/dialog_date_picker_confirm_tv"
                            app:layout_constraintEnd_toStartOf="@id/dialog_date_picker_confirm_tv"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
        </com.google.android.material.card.MaterialCardView>
